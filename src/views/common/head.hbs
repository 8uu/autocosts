<meta charset="UTF-8">
<meta name='viewport' content='width=device-width, initial-scale=1.0, user-scalable=0' >

<meta name="author" content="Autocosts Org" />
<meta name="viewport" content="width=device-width" />
<meta name="robots" content="{{#if clientData.isThisATest}}noindex, nofollow{{else}}index, follow{{/if}}" />

<meta property="og:type" content="website" />
<meta property="og:url" content="{{{clientData.urlHref}}}" />
<meta property="og:site_name" content="autocosts.info" />
<meta property="og:locale" content="{{{ getLocale }}}" />

<!-- /XX type pages -->
{{#if isGetCC}}
    <title>{{{ words.web_page_adapted_title }}}</title>

    <meta property="og:title" content="{{{ words.web_page_adapted_title }}}" />
    <meta property="og:description" content="{{{ words.socialmedia_description }}}" />

    <meta name="description" content="{{{ words.socialmedia_description }}}" />
    <meta name="keywords" content="{{{ words.meta_keywords }}}" />

    <!-- Banner to link to the APP, uncomment when APP is also upgraded to new UI
    <meta name="google-play-app" content="app-id=info.autocosts">
    <link href="/css/smart-app-banner.css" rel="stylesheet" media="screen" />
    -->

<!-- /stats page -->
{{else if isStats}}
    <title>World statistics of the Car Costs</title>

    <meta property="og:title" content="World statistics of Car Costs - autocosts.info" />
    <meta property="og:description" content="World statistics of the Car Costs for different countries" />
    <meta name="description" content="World statistics of the Car Costs for different countries" />

    <link rel="stylesheet" href="/css/stats/main.css" type="text/css">

<!-- /list page -->
{{else if isList}}
    <title>List of Countries for the Car Costs Calculator</title>

    <meta property="og:title" content="List of Countries for the Car Costs Calculator at autocosts.info" />
    <meta property="og:description" content="List of different Countries for the Car Costs Calculator" />
    <meta name="description" content="List of different Countries for the Car Costs Calculator" />

    <link rel="stylesheet" href="/css/list/main.css" type="text/css">
{{/if}}

