<div class="container">
    <div class="steps">
        <ul>
          <li class="list active number one"> <span>{{{fixed_costs}}}</span> 
            <ul>
                <li class="active done"><span>{{{depreciation}}}</span></li>
                <li class="active"><span>{{{insurance}}}</span></li>
                <li><span>{{{credit}}}</span></li>
                <li><span>{{{inspection}}}</span></li>
                <li><span>{{{road_taxes}}}</span></li>
            </ul>
          </li>
          <li class="list number two"><span>{{{running_costs}}}</span>
            <ul>
                <li><span>{{{fuel}}}</span></li>
                <li><span>{{{maintenance}}}</span></li>
                <li><span>{{{rep_improv}}}</span></li>
                <li><span>{{{parking}}}</span></li>
                <li><span>{{{tolls}}}</span></li>
                <li><span>{{{fines}}}</span></li>
                <li><span>{{{washing}}}</span></li>
            </ul>
          </li>
          <li class="list number three"><span>{{{extra_data1}}}</span>
            <ul>
                <li><span>{{{extra_data_public_transp}}}</span></li>
                <li><span>{{{extra_data_income}}}</span></li>
                <li><span>{{{extra_data_working_time}}}</span></li>
                <li><span>{{{distance}}}Distance</span></li>
                <li><span>{{{extra_data_time_spent_in_driving}}}</span></li>
            </ul>
          </li>
          
        </ul>
    </div>

            
    <!--  Start form -->
    <div class="form_part">
        <div class="form_part_head_title">
            <h2 class="lang-fixed_costs_header_1"> 
               1. {{{fixed_costs}}}
            </h2>
            <p class="lang-fixed_costs_header_2">
                {{{fixed_costs_header_2}}}
             </p>
        </div>
        
        <div class="field_container" id="depr_input_td">
            <label>
                {{{aq_date}}}
            </label>
            <input type="date" data-placeholder="DD/MM/AAAA" required aria-required="true" step="1" id="acquisitionDate" name="acquisitionDate" >

            <div class="next">
                <a href="#" class="button btn-orange">Next &#10230;</a>
                <span>press <b>Enter</b> or <b>Tab</b> </span>
            </div>
        </div>

        <div class="field_container" id="p2">
            <label>
                {{{com_value}}}
            </label>
            <div class="input_value">
                <input type="tel" min="0" step="Any" id="commercialValueAtAcquisition" class="currencyInput unit_left" name="commercialValueAtAcquisition">
                <span class="unit">
                    {{{curr_symbol}}}
                </span>
            </div>


            <div class="next">
                <a href="#" class="button btn-orange">Next &#10230;</a>
                <span>press <b>Enter</b> or <b>Tab</b> </span>
            </div>
        </div>

        <div class="field_container" id="p2">
            <label>
                {{{com_value_today}}}
            </label>
            <div class="input_value">
                <input type="tel" min="0" step="Any" id="commercialValueAtNow" class="currencyInput unit_left" name="commercialValueAtNow">
                <span class="unit">
                    {{{curr_symbol}}}
                </span>
            </div>


            <div class="next">
                <a href="#" class="button btn-orange">Next &#10230;</a>
                <span>press <b>Enter</b> or <b>Tab</b> </span>
            </div>
        </div>


        <!--************************** INSURANCE **************************-->
        <div class="field_container" id="">
            <label>
                {{{insurance}}}
            </label>
            <div class="radio_tabs">
                <div class="radio_tab_container">
                    <input type="radio" name="insurancePaymentPeriod" value="mensal">
                    <label>
                        {{{monthly}}}
                    </label>
                 </div>

                <div class="radio_tab_container">
                    <input type="radio" name="insurancePaymentPeriod" value="trimestral">
                    <label>
                        {{{trimesterly}}}
                    </label>
                </div>
                <div class="radio_tab_container">
                    <input type="radio" name="insurancePaymentPeriod" value="semestral">
                    <label>
                        {{{semesterly}}}
                    </label>
                </div>

                <div class="radio_tab_container">
                    <input type="radio" name="insurancePaymentPeriod" value="anual">
                    <label>
                        {{{yearly}}}
                    </label>
                </div>
            </div>

            <div class="input_value">
                <input type="tel" min="0" step="Any" id="insuranceValue" class="currencyInput unit_left" name="insuranceValue" />
                <span class="unit">
                    {{{curr_symbol}}}
                </span>
            </div>

            <div class="next">
                <a href="#" class="button btn-orange">Next &#10230;</a>
                <span>press <b>Enter</b> or <b>Tab</b> </span>
            </div>
        </div>

        <!--************************** CREDIT **************************-->
        <div class="field_container" id="">
            <label class="float_left">
                {{{insurance}}}
            </label>
            <div class="radio_tabs small_tabs">
                <div class="radio_tab_container">
                    <input type="radio" id="cred_auto_true" name="AutoCreditRadioBtn" value="true" class="" onclick="onclick_div_show('#sim_credDiv',true);">
                    <label>
                        {{{yes}}}
                    </label>
                 </div>

                <div class="radio_tab_container">
                    <input type="radio" id="cred_auto_false" name="AutoCreditRadioBtn" value="false" class="" onclick="onclick_div_show('#sim_credDiv',false);" checked>
                    <label>
                        {{{no}}}
                    </label>
                </div>
            </div>

            <div id="sim_credDiv">
                <label>
                    {{{credit_loan}}}
                </label>
                <div class="input_value small_input">
                     <input type="tel" min="0" step="Any" id="borrowedAmount" class="currencyInput unit_left" name="borrowedAmount"/>
                    <span class="unit" style="top: 49px">
                        {{{curr_symbol}}}
                    </span>
                </div>

                <label>
                    {{{credit_period}}}
                </label>
                <div class="input_value small_input">
                     <input type="tel" min="1" step="1" id="numberInstallments" name="numberInstallments" class="no_padding_left input_200" />
                </div>

                <label>
                    {{{credit_average_value}}}
                </label>
                <div class="input_value small_input">
                     <input type="tel" min="0" step="Any" id="amountInstallment" class="currencyInput unit_left" name="amountInstallment" />
                    <span class="unit">
                        {{{curr_symbol}}}
                    </span>
                </div>

                <label>
                    {{{credit_residual_value}}}
                </label>
                <div class="input_value small_input ">
                    <input type="tel" min="0" step="Any" id="residualValue" class="currencyInput unit_left" name="residualValue" />
                    <span class="unit" style="top: 49px">
                        {{{curr_symbol}}}
                    </span>
                </div>
            </div>

            <div class="next">
                <a href="#" class="button btn-orange">Next &#10230;</a>
                <span>press <b>Enter</b> or <b>Tab</b> </span>
            </div>
        </div>


        <!--************************** INSPECTION **************************-->
        <div class="field_container" id="">
            <label>
                {{{inspection}}}
            </label>

            <div id="sim_credDiv">
                <div class="input_value small_input">
                    <label>
                        {{{inspection_nbmr_times}}}
                    </label>
                     <input type="tel" min="0" step="1" id="numberInspections" name="numberInspections" class="no_padding_left input_70" />
                </div>

                <label class="disabled">
                    {{{inspection_price}}}
                </label>
                <div class="input_value small_input">
                     <input type="tel" min="0" step="Any" id="averageInspectionCost" class="currencyInput unit_left" name="averageInspectionCost" disabled />
                     <span class="unit">
                        {{{curr_symbol}}}
                    </span>
                </div>
            </div>

            <div class="next">
                <a href="#" class="button btn-orange">Next &#10230;</a>
                <span>press <b>Enter</b> or <b>Tab</b> </span>
            </div>
        </div>


        <!--************************** ROAD TAX **************************-->
        <div class="field_container" id="p2">
            <label>
                {{{road_taxes}}}
                <span>
                    {{{road_taxes_value}}}
                </span>
            </label>
            <div class="input_value">
                <input type="tel" min="0" step="Any" id="roadTaxes" class="currencyInput unit_n_year_left input_300" name="roadTaxes"/>
                <span class="unit">
                    {{{curr_symbol}}}/{{{year}}}
                </span>
            </div>


            <div class="next">
                <a href="#" class="button btn-orange">Next &#10230;</a>
                <span>press <b>Enter</b> or <b>Tab</b> </span>
            </div>
        </div>

        <!--  end form Step 1  -->


        <!--  Start form Step 2  -->
        <div class="form_part_head_title">
            <h2 class="lang-fixed_costs_header_1"> 
                2. {{{running_costs}}}
            </h2>
            <p class="lang-fixed_costs_header_2">
                {{{running_costs_header_2}}}
             </p>
        </div>

        <!--************************** FUELS **************************-->
        <div class="field_container" id="depr_input_td">
            <label>
                {{{fuel_desc}}}
            </label>
            <div class="input_value small_input no_margin_bottom">
                <label>
                    {{{fuel_calc}}}
                </label>
                <div class="radio_tabs small_tabs">
                    <div class="radio_tab_container">
                        <input type="radio" name="calc_combustiveis" id="radio_fuel_euros" value="euros" onclick="fuelCalculationMethodChange('currency');" />
                        <label>
                            {{{curr_name_plural}}}
                        </label>
                     </div>

                    <div class="radio_tab_container">
                        <input type="radio" name="calc_combustiveis" id="radio_fuel_km" value="km" onclick="fuelCalculationMethodChange('distance');" />
                        <label>
                            {{{std_dist}}}
                        </label>
                    </div>
                </div>
            </div>

            <!-- /////// Option selected = Kms /////// 
            Here start >>> CONSIDERING YOU DRIVE TO WORK? YES/NO-->
            <div id="distance_div_form2" class="input_value small_input no_margin_bottom">
                <label>
                    {{{fuel_job_calc}}}
                </label>
                <div class="radio_tabs small_tabs">
                    <div class="radio_tab_container">
                        <input type="radio" name="car_job_form2" class="no_left_margin" id="car_job_form2_yes" value="true" onclick="carToJob(true);" />
                        <label>
                            {{{yes}}}
                        </label>
                     </div>

                    <div class="radio_tab_container">
                        <input type="radio" name="car_job_form2" class="no_left_margin" id="car_job_form2_no" value="false" onclick="carToJob(false);" />
                        <label>
                            {{{no}}}
                        </label>
                    </div>
                </div>
            </div>

            
            <!-- YES & NO options -->

            <!-- /////// Option selected = YES /////// -->
            <div class="" id="div_car_job_yes_form2" style="display: none;">
                <div class="input_value small_input">
                    <label>
                        {{{fuel_days}}}
                    </label>
                    <input type="tel" min="1" max="7" step="1" class="no_padding_left" id="car_to_work_number_days_week" name="dias_por_semana"  />
                     <span class="unit_days-per-week">
                        {{{days_per_week_short}}}
                    </span>
                    <!--<span class="per">
                        {{{word_per}}}
                    </span>
                    <select class="time_period" id="combustiveis_periodo_euro"></select>-->
                </div>

                <div class="input_value small_input">
                    <label>
                        {{{fuel_dist_home_job}}}
                    </label>
                    <input type="tel" min="0" step="Any" id="car_to_work_distance_home_work" class="no_padding_left" name="km_entre_casa_trabalho" />
                    <span class="unit_kms">
                        {{{std_dist}}}
                    </span>
                </div>

                <div class="input_value small_input">
                    <label>
                        {{{fuel_dist_no_job}}}
                    </label>
                    <input type="tel" min="0" step="Any" name="km_fds" id="car_to_work_distance_weekend" class="no_padding_left" />
                    <span class="unit_kms" style="margin-top: 17px">
                        {{{std_dist}}}
                    </span>
                </div>
            </div>

            <!-- /////// Option selected = NO /////// -->
            <div class="" id="div_car_job_no_form2">
                <div class="input_value small_input">
                    <label>
                        {{{fuel_dist}}}
                    </label>
                    <input type="tel" min="0" step="Any" name="km_por_mes" id="no_car_to_work_distance" class="no_padding_left" />
                     <span class="unit_kms">
                        {{{std_dist}}}
                    </span>
                    <span class="per">
                        {{{word_per}}}
                    </span>
                    <select class="time_period" id="combustivel_period_km"></select>
                </div>
            </div>

            <div class="fuel_efficiency">
                <div class="input_value small_input">
                    <label>
                        {{{fuel_car_eff}}}
                    </label>
                    <input type="tel" min="0" step="Any" class="no_padding_left" id="fuel_efficiency" name="consumo_auto" />
                    <span class="unit_litre_100kms">
                        {{{std_fuel_calc}}}
                    </span>
                </div>

                <div class="input_value small_input">
                    <label>
                        {{{fuel_price}}}
                    </label>
                    <input type="tel" min="0" step="Any" id="fuel_price" class="currencyInput small_input_unit_per_litre unit_left" name="fuel_price" />
                    <span class="unit">
                        {{{curr_symbol}}}/{{{std_volume_short}}}
                    </span>
                </div>
            </div>


            <!-- /////// Option selected = C$ /////// -->
            <div id="currency_div_form2" class="input_value">
                <input type="tel" min="0" step="Any" size="5" id="fuel_currency_value" class="currencyInput input_200 unit_left" name="combustiveis_euro" />
                <span class="unit">
                    {{{curr_symbol}}}
                </span>
                <span class="per">
                    {{{word_per}}}
                </span>
                <select class="time_period" id="combustiveis_periodo_euro"></select>

            </div>

            <div class="next">
                <a href="#" class="button btn-orange">Next &#10230;</a>
                <span>press <b>Enter</b> or <b>Tab</b> </span>
            </div>
        </div>


        <!--************************** MAINTENANCE **************************-->
        <div class="field_container">
            <label>
                {{{maintenance}}}
                <span>
                    {{{maintenance_desc}}}
                </span>
            </label>
            <div class="input_value">
                <input type="tel" min="0" step="Any" name="revisoes" id="maintenance" class="currencyInput unit_n_year_left input_300">
                <span class="unit">
                    {{{curr_symbol}}}/{{{year}}}
                </span>
            </div>


            <div class="next">
                <a href="#" class="button btn-orange">Next &#10230;</a>
                <span>press <b>Enter</b> or <b>Tab</b> </span>
            </div>
        </div>


        <!--************************** REPAIRS AND IMPROVEMENTS **************************-->
        <div class="field_container">
            <label>
                {{{rep_improv}}}
                <span>
                    {{{rep_improv_desc}}}
                </span>
            </label>
            <div class="input_value">
                <input type="tel" min="0" step="Any" name="reparacoes" id="repairs" class="currencyInput unit_n_year_left input_300">
                <span class="unit">
                    {{{curr_symbol}}}/{{{year}}}
                </span>
            </div>


            <div class="next">
                <a href="#" class="button btn-orange">Next &#10230;</a>
                <span>press <b>Enter</b> or <b>Tab</b> </span>
            </div>
        </div>


        <!--************************** PARKING **************************-->
        <div class="field_container">
            <label>
                {{{parking}}}
                <span>
                    {{{parking_desc}}}
                </span>
            </label>
            <div class="input_value">
                <input type="tel" min="0" step="Any" name="parqueamento" id="parking" class="currencyInput unit_n_month_left">
                <span class="unit">
                    {{{curr_symbol}}}/{{{month}}}
                </span>
            </div>


            <div class="next">
                <a href="#" class="button btn-orange">Next &#10230;</a>
                <span>press <b>Enter</b> or <b>Tab</b> </span>
            </div>
        </div>


        <!--************************** TOLLS **************************-->
        <div class="field_container">
            <label>
                {{{tolls}}}
                <span>
                    {{{tolls_desc}}}
                </span>
            </label>

            <div class="input_value small_input no_margin_bottom">
                <label>
                    {{{tolls_day_calc}}}
                </label>
                <div class="radio_tabs small_tabs">
                    <div class="radio_tab_container">
                         <input type="radio" id="tolls_daily_true" name="tolls_daily_radioBtn" value="true" onclick="tolls_daily(true);" />
                        <label>
                            {{{yes}}}
                        </label>
                     </div>

                    <div class="radio_tab_container">
                        <input type="radio" id="tolls_daily_false" name="tolls_daily_radioBtn" value="false" onclick="tolls_daily(false);" checked="checked" />
                        <label>
                            {{{no}}}
                        </label>
                    </div>
                </div>
            </div>

            <!-- /////// Option selected = No /////// -->
            <div id="daily_tolls_false_div" class="input_value" style="display: none">
                <label>
                    <span style="width: 450px">
                        {{{tolls_day_calc_desc}}}
                    </span>
                </label>
                <div class="input_value">
                    <input type="tel" min="0" step="Any" id="no_daily_tolls_value" class="currencyInput input_200 unit_left" name="no_daily_tolls_value" />
                    <span class="unit">
                        {{{curr_symbol}}}
                    </span>
                    <span class="per">
                        {{{word_per}}}
                    </span>
                     <select class="time_period" id="tolls_period_select"></select>
                </div>
            </div>


            <!-- /////// Option selected = YES /////// -->
            <div id="daily_tolls_true_div" class="input_value">
                <input type="tel" min="0" step="Any" id="daily_expense_tolls" class="currencyInput input_200 unit_left" name="daily_expense_tolls"/>
                <span class="unit">
                    {{{curr_symbol}}}
                </span>
                <span class="during">
                    {{{during}}}
                </span>
                <input type="tel" min='1' max='31' step='1' id="number_days_tolls" name="number_days_tolls"/>
                <span class="days_per_month">
                    {{{days}}}
                    {{{word_per}}}
                    {{{month}}}
                </span>

            </div>


            <div class="next">
                <a href="#" class="button btn-orange">Next &#10230;</a>
                <span>press <b>Enter</b> or <b>Tab</b> </span>
            </div>
        </div>


        <!--************************** FINES / TICKETS **************************-->
        <div class="field_container">
            <label>
                {{{fines}}}
                <span>
                    {{{fines_desc}}}
                </span>
            </label>

            <div class="input_value">
                <input type="tel" min="0" step="Any" id="tickets_value" class="currencyInput input_200 unit_left" name="tickets_value" />
                <span class="unit">
                    {{{curr_symbol}}}
                </span>
                <span class="per">
                    {{{word_per}}}
                </span>
                <select class="time_period" id="tickets_period_select"></select>

            </div>

            <div class="next">
                <a href="#" class="button btn-orange">Next &#10230;</a>
                <span>press <b>Enter</b> or <b>Tab</b> </span>
            </div>
        </div>


        <!--************************** WASHING **************************-->
        <div class="field_container">
            <label>
                {{{washing}}}
                <span>
                    {{{washing_desc}}}
                </span>
            </label>

            <div class="input_value">
                <input type="tel" min="0" step="Any" id="washing_value" class="currencyInput input_200 unit_left" name="washing_value" />
                <span class="unit">
                    {{{curr_symbol}}}
                </span>
                <span class="per">
                    {{{word_per}}}
                </span>
                <select class="time_period big-dropdown" id="washing_period_select"></select>

            </div>

            <div class="next">
                <a href="#" class="button btn-orange">Next &#10230;</a>
                <span>press <b>Enter</b> or <b>Tab</b> </span>
            </div>
        </div>
        <!--  end form Step 2  -->


        <!--  Start form Step 3 - Additional Data  -->
        <div class="form_part_head_title">
            <h2 class="lang-fixed_costs_header_1"> 
                3. {{{extra_data1}}}
            </h2>
        </div>


        <!--************************** PUBLIC TRANSPORTS **************************-->
        <div class="field_container" id="">
            <label>
                {{{extra_data_public_transp}}}
            </label>

            <div id="sim_credDiv">
                <div class="input_value small_input">
                    <label>
                        {{{extra_data_family_nbr}}}
                    </label>
                    <input type="tel" min="0" step="1" id="household_number_people" class="no_padding_left input_70" name="pessoas_agregado">
                </div>

                <label>
                    {{{extra_data_price_pass}}}
                </label>
                <div class="input_value small_input">
                    <input type="tel" min="0" step="Any" id="public_transportation_month_expense" class="currencyInput unit_left" name="preco_passe" />
                     <span class="unit">
                        {{{curr_symbol}}}
                    </span>
                </div>
            </div>

            <div class="next">
                <a href="#" class="button btn-orange">Next &#10230;</a>
                <span>press <b>Enter</b> or <b>Tab</b> </span>
            </div>
        </div>


        <!--************************** INCOME **************************-->
        <div class="field_container" id="fin_effort_Div_form3">
            <label>
                {{{extra_data_income_question}}}
            </label>
            <div class="radio_tabs" id="income_div_form3">
                <div class="radio_tab_container">
                    <input type="radio" name="radio_income" value="year" id="radio_income_year"/>
                    <label>
                        {{{year}}}
                    </label>
                 </div>

                <div class="radio_tab_container">
                    <input type="radio" name="radio_income" value="month" id="radio_income_month">
                    <label>
                        {{{month}}}
                    </label>
                </div>
                <div class="radio_tab_container">
                    <input type="radio" name="radio_income" value="week" id="radio_income_week" />
                    <label>
                        {{{week}}}
                    </label>
                </div>

                <div class="radio_tab_container">
                    <input type="radio" name="radio_income" value="hour" id="radio_income_hour" />
                    <label>
                        {{{hour}}}
                    </label>
                </div>
            </div>

            <div class="input_value">
                <input type="tel" min="0" step="Any" id="income" class="currencyInput input_300" name="income"/>
                <span class="unit">
                    {{{curr_symbol}}}
                </span>
            </div>

            <div class="next">
                <a href="#" class="button btn-orange">Next &#10230;</a>
                <span>press <b>Enter</b> or <b>Tab</b> </span>
            </div>
        </div>


        <!--************************** WORKING TIME **************************-->
        <div class="field_container" id="">
            <label class="float_left">
                {{{extra_data_working_time_question}}}
            </label>
            <div class="radio_tabs small_tabs">
                <div class="radio_tab_container">
                    <input type="radio" name="radio_work_time" id="working_time_yes_form3" value="true" />
                    <label>
                        {{{yes}}}
                    </label>
                 </div>

                <div class="radio_tab_container">
                    <input type="radio" name="radio_work_time" id="working_time_no_form3" value="false" />
                    <label>
                        {{{no}}}
                    </label>
                </div>
            </div>

            <!-- /////// Option selected = YES /////// -->
            <div id="working_time_input_form3" class="input_value">
                <input type="tel" min="1" max="12" step="1" id="time_month_per_year" class="input_70" name="time_month_per_year" />
                <span class="months_year">
                    {{{months}}}/{{{year}}}
                </span>
                <input type="tel" min="1" max="168" step="Any" id="time_hours_per_week" class="input_70" name="time_hours_per_week" />
