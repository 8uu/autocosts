
<div class="form_part" id="form_part1">
    <div class="form_part_head_title">
        <header>
            <span class="lang-fixed_costs_header_1">
                {{{fixed_costs_header_1}}}
            </span>
            <span class="lang-fixed_costs_header_2">
                {{{fixed_costs_header_2}}}
            </span>
        </header>
    </div>
    <!--************************** DEPRECIATION ***************************************************-->
    <span class="form_section_title">
        {{{depreciation}}}
    </span>
    <table>
        <tr>
            <td>
                {{{aq_date}}}
            </td>
            <td id="depr_input_td">
                <input type="number" min="1" max="12" step="1" id="acquisitionMonth" name="acquisitionMonth" size="3" value="01">
                &#8209;<!--non breakable hyphen-->
                <input type="number" min="1910" step="1" id="acquisitionYear" name="acquisitionYear" size="6" value="2000">
            </td>
        </tr>
        <tr>
            <td class="p2">
                {{{com_value}}}
            </td>
            <td>
                <input type="number" min="0" step="Any" id="commercialValueAtAcquisition" class="currencyInput" name="commercialValueAtAcquisition" size="10">
                {{{curr_symbol}}}
            </td>
        </tr>
        <tr>
            <td class="p2">
                {{{com_value_today}}}
            </td>
            <td>
                <input type="number" min="0" step="Any" id="commercialValueAtNow" class="currencyInput" name="commercialValueAtNow" size="10">
                {{{curr_symbol}}}
            </td>
        </tr>
    </table>
    <!--************************** INSURANCE ***************************************************-->
    <span class="form_section_title">
        {{{insurance}}}
    </span>
    <table>
        <tr>
            <td>
                <div class="col">
                    <input type="radio" name="insurancePaymentPeriod" value="mensal">
                    <span>
                        {{{monthly}}}
                    </span>
                </div>
                <div class="col">
                    <input type="radio" name="insurancePaymentPeriod" value="trimestral">
                    <span>
                        {{{trimesterly}}}
                    </span>
                </div>
                <div class="col">
                    <input type="radio" name="insurancePaymentPeriod" value="semestral">
                    <span>
                        {{{semesterly}}}
                    </span>
                </div>
                <div class="col">
                    <input type="radio" name="insurancePaymentPeriod" value="anual">
                    <span>
                        {{{yearly}}}
                    </span>
                </div>
            </td>
            <td>
                <input type="number" min="0" step="Any" id="insuranceValue" class="currencyInput" name="insuranceValue" size="9" />
                {{{curr_symbol}}}
            </td>
        </tr>
    </table>
    <!--************************** CREDIT ***************************************************-->
    <span class="form_section_title">
        {{{credit}}}
    </span>
    <table>
        <tr>
            <td>
                {{{credit_question}}}
            </td>
            <td>
                <span class="nowrap">
                    <input type="radio" id="cred_auto_true" name="AutoCreditRadioBtn" value="true" class="no_left_margin" onclick="onclick_div_show('#sim_credDiv',true);">
                    <span class="set_right_margin">
                        {{{yes}}}
                    </span>
                </span>
                <span class="nowrap">
                    <input type="radio" id="cred_auto_false" name="AutoCreditRadioBtn" value="false" class="no_left_margin" onclick="onclick_div_show('#sim_credDiv',false);" checked>
                    <span>
                        {{{no}}}
                    </span>
                </span>
            </td>
        </tr>
    </table>
    <div id="sim_credDiv">
        <table>
            <tr>
                <td>
                    {{{credit_loan}}}
                </td>
                <td>
                    <input type="number" min="0" step="Any" id="borrowedAmount" class="currencyInput" name="borrowedAmount" size="9" />
                    {{{curr_symbol}}}
                </td>
            </tr>
            <tr>
                <td>
                    {{{credit_period}}}
                </td>
                <td>
                    <input type="number" min="1" step="1" id="numberInstallments" name="numberInstallments" size="9" />
                    {{{months}}}
                </td>
            </tr>
            <tr>
                <td>
                    {{{credit_average_value}}}
                </td>
                <td>
                    <input type="number" min="0" step="Any" id="amountInstallment" class="currencyInput" name="amountInstallment" size="9" />
                    {{{curr_symbol}}}/{{{month}}}
                </td>
            </tr>
            <tr>
                <td>
                    {{{credit_residual_value}}}
                </td>
                <td>
                    <input type="number" min="0" step="Any" id="residualValue" class="currencyInput" name="residualValue" size="9" />
                    {{{curr_symbol}}}
                </td>
            </tr>
        </table>
    </div>
    <!--************************** INSPECTION ***************************************************-->
    <span class="form_section_title">
        {{{inspection}}}
    </span>
    <table>
        <tr>
            <td>
                {{{inspection_nbmr_times}}}
            </td>
            <td>
                <input type="number" min="0" step="1" id="numberInspections" name="numberInspections" size="6" />
                {{{word_times}}}
            </td>
        </tr>
        <tr id="InspectionCost_tr">
            <td>
                {{{inspection_price}}}
            </td>
            <td>
                <input type="number" min="0" step="Any" id="averageInspectionCost" class="currencyInput" name="averageInspectionCost" size="6" />
                {{{curr_symbol}}}
            </td>
        </tr>
    </table>
    <!--************************** ROAD TAX ***************************************************-->
    <span class="form_section_title">
        {{{road_taxes}}}
    </span>
    <table>
        <tr>
            <td class="p2">
                {{{road_taxes_value}}}
            </td>
            <td>
                <input type="number" min="0" step="Any" id="roadTaxes" class="currencyInput" name="roadTaxes" size="6" />
                {{{curr_symbol}}}/{{{year}}}
            </td>
        </tr>
    </table>
    <div class="b-bottom">
        <span class="step">1/3</span>
        <button id="form_part1_button_next" class="button" onclick="openForm_part(1, 2)">
            &raquo;
        </button>
    </div>
</div>
<!-- end of form_part1-->


