<meta charset="UTF-8">
<meta name='viewport' content='width=device-width, initial-scale=1.0, user-scalable=0' >

<meta name="author" content="Autocosts Org" />
<meta name="viewport" content="width=device-width" />
<meta name="robots" content="{{#if clientData.isThisATest}}noindex, nofollow{{else}}index, follow{{/if}}" />

<meta property="og:type" content="website" />
<meta property="og:url" content="{{{clientData.fullURL}}}" />
<meta property="og:site_name" content="autocosts.info" />
<meta property="og:image" content="{{{clientData.basicURL}}}/img/logo/logo.svg" /> 
<meta property="og:locale" content="{{{ getLocale }}}" />

<!-- /XX type pages -->
{{#if isGetCC}}
    <title>{{{ getAdaptedTitle }}}</title>
   
    <meta property="og:title" content="{{{ getAdaptedTitle }}}" />
    <meta property="og:description" content="{{{ getMetaDescription }}}" />    
   
    <meta name="description" content="{{{ getMetaDescription }}}" />
    <meta name="keywords" content="{{{ getKeywords }}}" />        

    <meta name="google-play-app" content="app-id=info.autocosts">

    <link href="css/smart-app-banner.css" rel="stylesheet" media="screen" />

<!-- /stat page -->
{{else if isStats}}
    <title>World statistics of the Car Costs</title>  
    
    <meta property="og:title" content="World statistics of Car Costs" />
    <meta property="og:description" content="World statistics of the Car Costs for different countries" />
    
    <link rel="stylesheet" href="css/stats/main.css" type="text/css">

<!-- /list page -->
{{else if isList}}
    <title>List of Countries for the Car Costs Calculator</title>  
    
    <meta property="og:title" content="List of Countries for the Car Costs Calculator" />
    <meta property="og:description" content="List of Countries for the Car Costs Calculator" />
    
    <link rel="stylesheet" href="css/list/main.css" type="text/css">
{{/if}}

<link rel="stylesheet" href="css/mergedInit.css" media="screen" />

